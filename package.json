{
  "name": "mini-rest-api",
  "version": "1.0.0",
  "description": "API RESTful con Node.js, Express y Prisma",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "generate": "npx prisma generate",
    "seed": "npx ts-node prisma/seed.ts",
    "start-all": "npm run generate && npm run seed && npm run dev",
    "test": "jest --forceExit --detectOpenHandles"
  },
  "keywords": ["rest", "api", "nodejs", "express", "prisma", "postgresql"],
  "author": "Ramiro Gonz√°lez",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^4.16.2",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "pg": "^8.17.2",
    "prisma": "^4.16.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.10",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}